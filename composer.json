{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "aloha/twilio": "^4.0",
        "arcanedev/seo-helper": "1.5",
        "bkwld/sitemap-from-routes": "^1.0",
        "cviebrock/eloquent-sluggable": "4.5",
        "devio/permalink": "^0.6.1",
        "eluceo/ical": "^0.15.0",
        "facebook/graph-sdk": "^5.7",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "jrm2k6/cloudder": "^0.6.0",
        "lab404/laravel-impersonate": "^1.2",
        "laravel/cashier": "~7.0",
        "laravel/cashier-braintree": "~2.0",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "~2.0.0",
        "laravel/tinker": "^1.0",
        "laravelium/sitemap": "2.8.*",
        "mad-web/laravel-seoable": "^1.2",
        "mpociot/laravel-apidoc-generator": "^3.7",
        "paypal/rest-api-sdk-php": "^1.14",
        "pusher/pusher-http-laravel": "^4.2",
        "renatomarinho/laravel-page-speed": "^1.8",
        "srmklive/paypal": "~1.0",
        "tymon/jwt-auth": "^0.5.12",
        "vluzrmos/collective-html": "^1.0",
        "yajra/laravel-disqus": "^1.1"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
